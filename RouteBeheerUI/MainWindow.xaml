<Window x:Class="RouteBeheerUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RouteBeheerUI"
        mc:Ignorable="d"
        Title="LocationManager" Height="110" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Content="Add Location" Margin="5" Click="ButtonAdd_Click"/>
        <Button Grid.Row="1" Content="Update Location" Margin="5" Click="ButtonUpdate_Click"/>
    </Grid>
</Window>
